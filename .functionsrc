function search() {
	grep -rin --exclude-dir="vendor" . -e $1
}

function scuttle() {
	docker kill $(docker ps -a -q)
	docker rm $(docker ps -a -q)
}

function sink() {
	docker rmi $(docker images -q)
}

function gitt() {
	git add -A
	git commit -m $1
	git push
}

function gitt() {
	if [ -z "$1" ]
	then
		echo "No commit message supplied"
	else
		git add -A
		git commit -m $1
		git push
	fi
}

function branch() {
	if [ -z "$1" ]
	then
		echo "No repo supplied"
	else
		cd $GOPATH/src/github.com/byuoitav/$1
		pwd
		git branch | grep \*
		cd -
	fi
}
