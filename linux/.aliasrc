alias gin="go run server.go"
alias tunnel="ngrok http 8000"
alias ode="npm install && node main.js"
alias test="go test -v -cover ./..."
alias scuttle="bash ~/.dotfiles/docker-scuttle.sh"
alias sink="bash ~/.dotfiles/docker-sink.sh"
alias float="docker-compose build && docker-compose up -d"
alias inflate="scuttle && float"
alias pun="pico-build run"
alias ds="dropbox status"
alias polybar="vim ~/.dotfiles/linux/.config/polybar/config"
alias i3="vim ~/.dotfiles/linux/.config/i3/config"
