alias gin="go run server.go"
alias test="go test -v -race ./..."
alias ode="npm install && node main.js"
alias float="docker-compose build && docker-compose up -d"
alias inflate="scuttle && float"
alias pun="pico-build run"
alias ped="pico-build edit"
alias ds="dropbox-cli status"
alias orphans="sudo pacman -Rns $(pacman -Qtdq)"
alias updateapi="cd $GOPATH/src/github.com/byuoitav/av-api && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/telnet-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/crestron-control-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/pjlink-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/configuration-database-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/sony-control-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/london-audio-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/cgi-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/av-api-rpc && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/touchpanel-ui-microservice && git checkout master && git pull"
alias devup="cd $GOPATH/src/github.com/byuoitav/av-api && docker-compose -f docker-compose-dev.yml build && docker-compose -f docker-compose-dev.yml up -d && cd -"
alias devdown="docker-compose -f $GOPATH/src/github.com/byuoitav/av-api/docker-compose-dev.yml down"
alias produp="docker-compose -f $GOPATH/src/github.com/byuoitav/av-api/docker-compose-prod.yml pull && docker-compose -f $GOPATH/src/github.com/byuoitav/av-api/docker-compose-prod.yml up -d"
alias proddown="docker-compose -f $GOPATH/src/github.com/byuoitav/av-api/docker-compose-prod.yml down"
