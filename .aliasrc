alias pun="pico-build run"
alias ped="pico-build edit"
alias ds="dropbox-cli status"
alias orphans="sudo pacman -Rns $(pacman -Qtdq)"
alias updateapi="cd $GOPATH/src/github.com/byuoitav/av-api && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/crestron-control-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/pjlink-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/configuration-database-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/sony-control-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/london-audio-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/cgi-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/av-api-rpc && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/touchpanel-ui-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/configuration-database-tool && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/pulse-eight-neo-microservice && git checkout master && git pull && cd $GOPATH/src/github.com/byuoitav/adcp-control-microservice && git checkout master && git pull"
alias localup="cd $GOPATH/src/github.com/byuoitav/av-api && docker-compose -f docker-compose-build.yml build && docker-compose -f docker-compose-build.yml up -d && cd -"
