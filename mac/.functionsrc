function search() {
	grep -rin . -e $1
}

function scuttle() {
	docker kill $(docker ps -a -q)
	docker rm $(docker ps -a -q)
}

function sink() {
	docker rmi $(docker images -q)
}

function gitt() {
	if [ -z "$1" ]
	then
		echo "No commit message supplied"
	else
		git add -A
		git commit -m $1
		git push
	fi
}
